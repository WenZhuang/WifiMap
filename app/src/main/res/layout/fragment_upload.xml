<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="com.bupt.wifimap.UploadFragment">

    <!-- TODO: Update blank fragment layout -->
    <Button
        android:id="@+id/manuUpload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Button1" />

    <Button
        android:id="@+id/autoUpload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/manuUpload"
        android:text="@string/Button2" />

    <Button
        android:id="@+id/stop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/autoUpload"
        android:layout_alignBottom="@+id/autoUpload"
        android:text="@string/Button3" />

</RelativeLayout>
